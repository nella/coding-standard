		"consistence/coding-standard": "~0.10.0",
		"slevomat/coding-standard": "~1.0.4"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2"
	},
	"config": {
		"bin-dir": "bin"
	},
	"archive": {
