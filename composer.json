{
	"name": "nella/coding-standard",
	"description": "Nella Project - Coding Standard - PHP Code Sniffer rules",
	"keywords": ["coding standard", "cs", "coding", "standard", "ruleset", "phpcs", "PHPCodeSniffer", "CodeSniffer", "sniffer"],
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/nella/coding-standard",
	"authors": [
		{
			"name": "Patrik Votoček",
			"email": "patrik@votocek.cz",
			"homepage": "http://patrik.votocek.cz",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/nella/coding-standard/issues",
		"source": "https://github.com/nella/coding-standard"
	},
	"autoload": {
		"psr-4": {
			"Nella\\Sniffs\\": [ "Nella/Sniffs" ],
			"Consistence\\Sniffs\\": [ "Consistence/Sniffs" ]
		}
	},
	"require": {
		"php": ">=5.5.0",
		"squizlabs/php_codesniffer": "~2.3.4"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "~0.3.2",
		"jakub-onderka/php-parallel-lint": "~0.9.0"
	},
	"config": {
		"bin-dir": "bin"
	},
	"archive": {
		"exclude": [
			".gitattributes",
			".gitignore",
			".travis.yml"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
